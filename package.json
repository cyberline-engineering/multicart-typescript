{
    "name": "@cyberline-engineering/multicartshop-client",
    "description": "Multicartshop API typescript client",
    "homepage": "https://www.multicartshop.com",
    "private": false,
    "version": "0.2.0",
    "repository": "github:cyberline-engineering/multicart-typescript",
    "main": "./dist/multicartshop-client.cjs",
    "module": "./dist/multicartshop-client.mjs",
    "types": "./dist/multicartshop-client.d.ts",
    "exports": {
        ".": {
            "require": "./dist/multicartshop-client.cjs",
            "import": "./dist/multicartshop-client.mjs"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:dev": "vite build",
        "preview": "vite preview",
        "test": "cross-env GLOBAL_AGENT_HTTP_PROXY=http://127.0.0.1:8888 NODE_TLS_REJECT_UNAUTHORIZED=0 jest --forceExit --runInBand",
        "oa:generate": "cross-env TS_POST_PROCESS_FILE=\"$pwd\\node_modules\\.bin\\prettier.cmd --write\" openapi-generator-cli generate -i https://stage.redoc.cledeploy.com/openapi/v1/openapi.json -g typescript-fetch -o openapi/generated -c openapi/oa-config.json",
        "oa:generate:dev": "openapi-generator-cli generate -i http://localhost:5001/openapi/v1/openapi.json -g typescript-fetch -o openapi/generated -c openapi/oa-config.json"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.5.2",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.3",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "cross-env": "^7.0.3",
        "cross-fetch": "^3.1.5",
        "debugger-is-attached": "^1.2.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "global-agent": "^3.0.0",
        "jest": "^29.1.2",
        "oidc-client-ts": "^2.0.6",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "vite": "^3.1.7",
        "vite-plugin-dts": "^1.6.5"
    },
    "dependencies": {
        "@cyberline-engineering/cle-oidc-client": "^0.2.8"
    },
    "engines": {
        "node": ">=16",
        "npm": ">=7"
    }
}
